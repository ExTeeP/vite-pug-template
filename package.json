{
  "name": "vite-bem-template",
  "version": "1.0.4",
  "type": "module",
  "description": "simple bem pug+scss template",
  "author": "Alexey Kozyutenko",
  "license": "MIT",
  "scripts": {
    "dev": "gulp pugMixins && vite",
