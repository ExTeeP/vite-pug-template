    "eslint": "8.37.0",
    "gulp": "4.0.2",
    "pug-lint": "^2.7.0",
    "sass": "1.61.0",
    "stylelint": "15.10.0",
    "stylelint-config-htmlacademy": "2.0.4",
    "stylelint-scss": "5.0.0",
    "vite": "4.2.1",
    "vite-plugin-babel": "1.1.3",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-imagemin": "0.6.1",
    "vite-plugin-multipage": "1.0.0",
    "vite-plugin-pug-transformer": "1.0.3",
    "vite-plugin-sass-glob-import": "2.0.0",
    "vite-plugin-stylelint": "4.3.0",
